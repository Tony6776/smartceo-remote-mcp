{
  "version": 1,
  "framework": {
    "name": "custom",
    "version": "1.0"
  },
  "imageSettings": {
    "sizes": [100, 200, 400, 800, 1200, 1600]
  },
  "routes": [
    {
      "path": "/mcp/*",
      "target": "/",
      "type": "rewrite"
    },
    {
      "path": "/*",
      "target": "/",
      "type": "rewrite"
    }
  ],
  "computeResources": [
    {
      "name": "default",
      "runtime": "nodejs18.x",
      "entrypoint": "index.js"
    }
  ]
}
